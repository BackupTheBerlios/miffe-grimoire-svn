inherit  perl--modules
   VERSION=( "3.50" "3.48" "3.46" "3.45" "3.42" "3.40" "3.36" )
    SOURCE="G/GA/GAAS/${SPELL#perl--}-$VERSION.tar.gz"
  HOMEPAGE="$HOMEPAGE/search?dist=HTML-Parser"
       REQ="perl perl--HTML-Tagset"
  ESTIMATE="40"
      DESC="$SPELL provides modules that parse and extract information from HTML documents"

build() {
  yes  |  perl_module_build
}
