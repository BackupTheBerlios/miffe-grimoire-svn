     ATTRIBUTE="x11 new"
        SOURCE="rawstudio-$VERSION.tar.gz"
           URL="http://rawstudio.org/files/release/"
      HOMEPAGE="http://rawstudio.org"
	   REQ="gtk+ GConf"
