inherit  perl--modules
   VERSION=( "0.44" )
    SOURCE="I/IN/INGY/${SPELL#perl--}-$VERSION.tar.gz"
  HOMEPAGE="$HOMEPAGE/search?dist=Inline"
       REQ=( "perl  perl--Digest-MD5  perl--Parse-RecDescent" )
  ESTIMATE="49"
      DESC="$SPELL allows to write Perl subroutines in other programming languages."

build() {
  yes  |  perl_module_build
}
